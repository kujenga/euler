#include <stdio.h>
#include <stdlib.h>

int main( int argc, char** argv )
{
  char* num = argv[1];
  int n = atoi(num);
  for ( int x = 0; x*x < n; x++ ) {
    for ( int y = 0; y*y < n; y++ ) {
      for ( int z = 0; z*z < n; z++ ) {
	if ( (x+y+z == n) && ((x*x + y*y) == (z*z)) ) {
	  
	}
      }
    }
  }
}
